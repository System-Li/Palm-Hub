-- Funktion 1
local function script1()
    while true do
        print("Script 1 läuft...")
        task.wait(1)
    end
end

-- Funktion 2
local function script2()
    while true do
        print("Script 2 läuft...")
        task.wait(2)
    end
end

-- Beide Funktionen gleichzeitig starten
